{
  "name": "wpai/agentwp",
  "description": "Adds a AI Agent to your WordPress installation.",
  "license": "GPL2",
  "autoload": {
    "psr-4": {
      "WpAi\\AgentWp\\": "server/"
    }
  },
  "authors": [
    {
      "name": "Greg Hunt",
      "email": "greg@codewp.ai"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "php-stubs/wordpress-stubs": "^6.4",
    "yahnis-elsts/plugin-update-checker": "^5.4"
  },
  "require": {
    "php": ">=7.4",
    "kucrut/vite-for-wp": "^0.8.0",
    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/http-foundation": "^5.0",
    "wpai-inc/awp-api-client": "dev-main"
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/wpai-inc/awp-api-client.git"
    }
